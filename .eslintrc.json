{
  "env": {
    "amd": true,
    "browser": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Promise": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 3
  },
  "rules": {
    "no-constant-condition": 0,
    "no-fallthrough": 0,
    "no-redeclare": 0,
    "no-useless-escape": 0
  }
}
